        <activity
            android:name=".activities.PagamentoOnline_3_1_Activity"
            android:parentActivityName=".activities.PagamentoOnline_2_Activity" />
        <activity
            android:name=".activities.PagamentoOnline_2_Activity"
            android:parentActivityName=".activities.PagamentoOnline_1_Activity" />
